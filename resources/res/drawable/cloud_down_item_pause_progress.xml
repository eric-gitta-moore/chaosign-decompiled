<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <solid android:color="@color/white"/>
            <stroke android:width="0.5dp" android:color="@color/white"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle">
                <gradient android:startColor="@color/color_999999" android:endColor="@color/color_999999" android:angle="90" android:centerY="0.75" android:centerColor="@color/color_999999"/>
            </shape>
        </clip>
    </item>
</layer-list>
