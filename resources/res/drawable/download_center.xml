<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dp"/>
            <gradient android:startColor="#e0e0e0" android:endColor="#e0e0e0" android:angle="270" android:centerY="0.75" android:centerColor="#e0e0e0"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <gradient android:startColor="#b2b2b2" android:endColor="#b2b2b2" android:angle="270" android:centerY="0.75" android:centerColor="#b2b2b2"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <gradient android:startColor="#0099ff" android:endColor="#0099ff" android:angle="270" android:centerY="0.75" android:centerColor="#0099ff"/>
            </shape>
        </clip>
    </item>
</layer-list>
