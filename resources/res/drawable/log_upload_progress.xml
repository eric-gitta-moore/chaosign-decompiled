<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <corners android:radius="150dp"/>
            <solid android:color="@color/bg_color_f5f6f8"/>
            <stroke android:width="0.5dp" android:color="@color/bg_color_f5f6f8"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="150dp"/>
                <gradient android:startColor="@color/chaoxing_blue" android:endColor="@color/chaoxing_blue" android:angle="90" android:centerY="0.75" android:centerColor="@color/chaoxing_blue"/>
            </shape>
        </clip>
    </item>
</layer-list>
