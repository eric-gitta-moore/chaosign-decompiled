<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <corners android:radius="10dp"/>
            <solid android:color="#ffffff"/>
            <stroke android:width="1px" android:color="@color/download_subscript_progress_broder"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle">
                <gradient android:startColor="#0592f0" android:endColor="#0592f0" android:angle="90" android:centerY="0.75" android:centerColor="#0592f0"/>
                <corners android:radius="10dp"/>
            </shape>
        </clip>
    </item>
</layer-list>
