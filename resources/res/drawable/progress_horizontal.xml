<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <stroke android:width="1dp" android:color="#c1c1c1"/>
            <solid android:color="#fff"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="#7dee6b" android:endColor="#2eb421" android:angle="270" android:centerY="0.5" android:centerColor="#2eb421"/>
                <stroke android:width="1dp" android:color="#18b818"/>
            </shape>
        </clip>
    </item>
</layer-list>
