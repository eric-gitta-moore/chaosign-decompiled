        <ImageView android:layout_gravity="left|center_vertical|center_horizontal|center" android:id="@+id/back_imageView" android:paddingLeft="15dp" android:paddingRight="15dp" android:layout_width="wrap_content" android:layout_height="match_parent" android:src="@drawable/bg_back"/>
