<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout android:id="@+id/topBarView" android:layout_width="match_parent" android:layout_height="wrap_content">
        <include android:visibility="invisible" layout="@layout/common_title_bar2_title_2line"/>
    </RelativeLayout>
    <LinearLayout android:orientation="vertical" android:id="@+id/llContainer" android:layout_width="match_parent" android:layout_height="wrap_content">
        <androidx.recyclerview.widget.RecyclerView android:id="@+id/gvClass" android:background="@color/white" android:paddingLeft="10dp" android:paddingRight="10dp" android:paddingBottom="11dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:overScrollMode="never"/>
    </LinearLayout>
    <View android:id="@+id/view" android:background="#80000000" android:layout_width="match_parent" android:layout_height="wrap_content"/>
</LinearLayout>
