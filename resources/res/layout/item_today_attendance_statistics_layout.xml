<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" android:gravity="center_vertical" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
    <LinearLayout android:gravity="center_vertical" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
        <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:baselineAligned="false">
            <LinearLayout android:gravity="left|center_vertical|center_horizontal|center|start" android:id="@+id/overTimeLayout" android:layout_width="match_parent" android:layout_height="wrap_content" android:weightSum="4" android:layout_weight="1">
                <TextView android:textSize="12sp" android:textColor="@color/ds_color_base_black" android:gravity="center" android:id="@+id/labelOverTime" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/label_over_time" android:layout_weight="1"/>
                <ProgressBar android:id="@+id/overTimePB" android:layout_width="wrap_content" android:layout_height="8dp" android:progressDrawable="@drawable/ds_daily_progress_bar_drawable" android:minWidth="70dp" android:layout_weight="2" android:layout_marginStart="10dp" style="@android:style/Widget.ProgressBar.Horizontal"/>
                <TextView android:textSize="12sp" android:textColor="@color/ds_color_base_gray" android:id="@+id/overTimeCountTv" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_weight="1" android:layout_marginStart="11.5dp"/>
            </LinearLayout>
            <LinearLayout android:gravity="left|center_vertical|center_horizontal|center|start" android:id="@+id/absentLayout" android:layout_width="match_parent" android:layout_height="wrap_content" android:weightSum="4" android:layout_weight="1">
                <TextView android:textSize="12sp" android:textColor="@color/ds_color_base_black" android:gravity="center" android:id="@+id/labelAbsent" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/label_absent" android:layout_weight="1"/>
                <ProgressBar android:id="@+id/absentPB" android:layout_width="wrap_content" android:layout_height="8dp" android:progressDrawable="@drawable/ds_daily_progress_bar_drawable" android:minWidth="70dp" android:layout_weight="2" android:layout_marginStart="10dp" style="@android:style/Widget.ProgressBar.Horizontal"/>
                <TextView android:textSize="12sp" android:textColor="@color/ds_color_base_gray" android:layout_gravity="start" android:id="@+id/absentCountTv" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_weight="1" android:layout_marginStart="11.5dp"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="14dp" android:baselineAligned="false">
            <LinearLayout android:gravity="left|center_vertical|center_horizontal|center|start" android:orientation="horizontal" android:id="@+id/lateLayout" android:layout_width="match_parent" android:layout_height="wrap_content" android:weightSum="4" android:layout_weight="1">
                <TextView android:textSize="12sp" android:textColor="@color/ds_color_base_black" android:gravity="center" android:id="@+id/labelLate" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/label_late" android:layout_weight="1"/>
                <ProgressBar android:id="@+id/latePB" android:layout_width="wrap_content" android:layout_height="8dp" android:progressDrawable="@drawable/ds_daily_progress_bar_drawable" android:minWidth="70dp" android:layout_weight="2" android:layout_marginStart="10dp" style="@android:style/Widget.ProgressBar.Horizontal"/>
                <TextView android:textSize="12sp" android:textColor="@color/ds_color_base_gray" android:layout_gravity="start" android:id="@+id/lateCountTv" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="0/1" android:layout_weight="1" android:layout_marginStart="11.5dp"/>
            </LinearLayout>
            <LinearLayout android:gravity="left|center_vertical|center_horizontal|center|start" android:id="@+id/goOutLayout" android:layout_width="match_parent" android:layout_height="wrap_content" android:weightSum="4" android:layout_weight="1">
                <TextView android:textSize="12sp" android:textColor="@color/ds_color_base_black" android:gravity="center" android:id="@+id/labelOut" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/label_out" android:layout_weight="1"/>
                <ProgressBar android:id="@+id/outPB" android:layout_width="wrap_content" android:layout_height="8dp" android:progressDrawable="@drawable/ds_daily_progress_bar_drawable" android:minWidth="70dp" android:layout_weight="2" android:layout_marginStart="10dp" style="@android:style/Widget.ProgressBar.Horizontal"/>
                <TextView android:textSize="12sp" android:textColor="@color/ds_color_base_gray" android:layout_gravity="start" android:id="@+id/outCountTv" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="10/11" android:layout_weight="1" android:layout_marginStart="11.5dp"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:gravity="left|center_vertical|center_horizontal|center|start" android:orientation="horizontal" android:id="@+id/earlyLayout" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="14dp" android:baselineAligned="false">
            <LinearLayout android:gravity="left|center_vertical|center_horizontal|center|start" android:layout_width="match_parent" android:layout_height="wrap_content" android:weightSum="4" android:layout_weight="1">
                <TextView android:textSize="12sp" android:textColor="@color/ds_color_base_black" android:gravity="center" android:id="@+id/labelLeaveEarly" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/label_leave_early" android:layout_weight="1"/>
                <ProgressBar android:id="@+id/leaveEarlyPB" android:layout_width="wrap_content" android:layout_height="8dp" android:progressDrawable="@drawable/ds_daily_progress_bar_drawable" android:minWidth="70dp" android:layout_weight="2" android:layout_marginStart="10dp" style="@android:style/Widget.ProgressBar.Horizontal"/>
                <TextView android:textSize="12sp" android:textColor="@color/ds_color_base_gray" android:layout_gravity="start" android:id="@+id/leaveEarlyCountTv" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_weight="1" android:layout_marginStart="11.5dp"/>
            </LinearLayout>
            <LinearLayout android:gravity="left|center_vertical|center_horizontal|center|start" android:id="@+id/leaveLayout" android:layout_width="match_parent" android:layout_height="wrap_content" android:weightSum="4" android:layout_weight="1">
                <TextView android:textSize="12sp" android:textColor="@color/ds_color_base_black" android:gravity="center" android:id="@+id/labelLeave" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/label_leave" android:layout_weight="1"/>
                <ProgressBar android:id="@+id/leavePB" android:layout_width="wrap_content" android:layout_height="8dp" android:progressDrawable="@drawable/ds_daily_progress_bar_drawable" android:minWidth="70dp" android:layout_weight="2" android:layout_marginStart="10dp" style="@android:style/Widget.ProgressBar.Horizontal"/>
                <TextView android:textSize="12sp" android:textColor="@color/ds_color_base_gray" android:layout_gravity="start" android:id="@+id/leaveCountTv" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_weight="1" android:layout_marginStart="11.5dp"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</merge>
