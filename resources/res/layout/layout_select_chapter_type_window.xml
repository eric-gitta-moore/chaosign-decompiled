<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:id="@+id/rlContainer" android:background="#80333333" android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout android:orientation="vertical" android:background="@color/white" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_alignParentBottom="true">
        <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
            <TextView android:textSize="15sp" android:textColor="@color/color_333333" android:gravity="center" android:id="@+id/tvSubChapter" android:paddingTop="15dp" android:paddingBottom="15dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/import_subject_chapter"/>
            <View android:background="#e6e6e6" android:layout_width="match_parent" android:layout_height="1px"/>
            <TextView android:textSize="15sp" android:textColor="@color/color_333333" android:gravity="center" android:id="@+id/tvCourseChapter" android:paddingTop="15dp" android:paddingBottom="15dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/import_course_chapter"/>
            <View android:background="#e6e6e6" android:layout_width="match_parent" android:layout_height="1px"/>
        </LinearLayout>
        <View android:background="@color/bg_color_f5f6f8" android:layout_width="match_parent" android:layout_height="6dp"/>
        <TextView android:textSize="15sp" android:textColor="@color/color_333333" android:gravity="center" android:id="@+id/tvCancel" android:paddingTop="15dp" android:paddingBottom="15dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="5dp" android:text="@string/comment_cancle"/>
    </LinearLayout>
</RelativeLayout>
