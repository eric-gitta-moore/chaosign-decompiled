<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:paddingTop="20dp" android:paddingBottom="20dp" android:layout_width="match_parent" android:layout_height="wrap_content">
    <RelativeLayout android:id="@+id/rl_timbre" android:layout_width="50dp" android:layout_height="50dp" android:layout_centerHorizontal="true">
        <com.makeramen.roundedimageview.RoundedImageView android:id="@+id/iv_timbre" android:layout_width="44dp" android:layout_height="44dp" android:layout_centerInParent="true" app:is_circle="true"/>
    </RelativeLayout>
    <TextView android:textSize="14sp" android:textColor="#1b1f26" android:id="@+id/tv_timbre_name" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="8dp" android:layout_below="@+id/rl_timbre" android:layout_centerHorizontal="true"/>
</RelativeLayout>
